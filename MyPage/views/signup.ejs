<!DOCTYPE html>
<html>
  <head>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css"
    />
    <link rel="stylesheet" href="css/style_signup.css" />
  </head>
  <body>
    <div class="container">
      <h1>
        <a href="/">
          <span id="title"><br />S K K U<br />M E A L</span>
        </a>
      </h1>
      
      <p></p>

      <div class="signupBox">
        
          <!-- body에 넣어서-->
          <!-- name -->
          <div class="d-flex">
            <label for="NAME_input">NAME</label>
            <input
              type="text"
              name="NAME_input"
              class="form-control"
              id="NAME_input"
              placeholder=""
            />
          </div>
          <!-- birth -->
          <div class="d-flex">
            <label for="BIRTH_input">BIRTH</label>
            <input
              type="text"
              class="form-control"
              id="BIRTH_input"
              name="BIRTH_input"
              placeholder=""
            />
          </div>
          <!-- ID -->
          <div class="d-flex">
            <label for="ID_input">ID</label>
            <input
              type="text"
              class="form-control"
              id="ID_input"
              name="ID_input"
              placeholder=""
            />
            <!--
            <input
              class="form-check-input"
              type="checkbox"
              value=""
              id="flexCheckDefault"
            />
            -->
          </div>
          <!-- password -->
          <div class="d-flex">
            <label for="PW_input">PWD</label>
            <input
              type="password"
              class="form-control"
              id="PW_input"
              name="PW_input"
              placeholder=""
            />
          </div>
          <!-- re-password -->
          
          <div class="d-flex">
            <label for="RPW_input">RE-PWD</label>
            <input
              type="password"
              class="form-control"
              id="RPW_input"
              name="RPW_input"
              placeholder=""
            />
            <!--
            <input
              class="form-check-input"
              type="checkbox"
              value=""
              id="flexCheckDefault"
            />
            -->
          </div>
          <!-- email -->
          <div class="d-flex">
            <label for="EMAIL_input">EMAIL</label>
            <input
              type="email"
              class="form-control"
              id="EMAIL_input"
              name="EMAIL_input"
              placeholder=""
            />
          </div>
          <button class ="btn" onclick="checkValid()">SIGN</button>
          
        
      </div>
    </div>
  <script type="text/javascript" src="./../javascripts/signup.js">
  </script>
  </body>
</html>
